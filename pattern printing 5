output===4

 16  12  8  4 

 12  9  6  3 

 8  6  4  2 

 4  3  2  1 











#include<stdio.h>
#include<conio.h>
int main(){
       int j,i,n;
       scanf("%d",&n);
       for(i=n; i>0;i--)
       {
              
             
              for(j=n;j>0;j--)
              {
             
              printf(" %d ",j*i);
              
       
              }
              
              printf("\n");
              
              
       }
        
        
       
       
       
       return 0;
}

#include<stdio.h>
int main(){
       int j,i,n;
       scanf("%d",&n);
       for(i=0; i<n;i++)
       {
              for(j=0; j<n;j++)
              {
              if(i==0||i==(n-1)||j==0||j==(n-1))
              printf("%d",1);
              
              else 
              printf("%d",0);
              }
              
              printf("\n");
              
              
       }
        
        
       
       
       
       return 0;
}5






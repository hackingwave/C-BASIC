#include<stdio.h>
#include<conio.h>
int main(){
       int j,i,n,k;
       scanf("%d",&n);
       for(i=n; i>0;i--)
       {
              
              for(j=i;j<=n;j++)
              {
             
              printf("%d", j);
              
              }
              for(k=1;k<i;k++)
              {
                     
                    printf("%d", n);  
              }
              
             
              printf("\n");
       }
        
      
       
       return 0;
}

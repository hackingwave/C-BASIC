//// printing difference of 5
output====

7 12
7 2
3  8
5 10
8 3

input===
#include<stdio.h>
#include<math.h>
#include<stdlib.h>
int main(){
     int A[14]={1,7,3,5,8,12,13,15,27,2,11,9,10,16};
     
       for(int i=0;i<14;i++)
       {
              
              for( int j=0;j<14;j++)
              {
                     if((abs(A[i]-A[j]))==5)
                     printf("%d %d",A[i],A[j]);
                     printf("\n");
              }
              
       }
      
       return  0;
}


























2 7





































































































































output===4

 2   5   10   17  

 5   8   13   20  

 10   13   18   25  

 17   20   25   32  












#include<stdio.h>
#include<conio.h>
int main(){
       int j,i,n;
       scanf("%d",&n);
       for(i=1; i<=n;i++)
       {
              
              for(j=1;j<=n;j++)
              {
             
              printf(" %d  ", (i*i)+(j*j));
              
       
              }
              
             
              printf("\n");
       }
        
      
       
       return 0;
}

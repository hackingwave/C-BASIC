#include<stdio.h>
#include<conio.h>
int main(){
       int j,i,n;
       scanf("%d",&n);
       for(i=0; i<n;i++)
       {
              
              for(j=0;j<i;j++)
              {
             
              printf("%c", (64+n) +j -i);
              
       
              }
              
             
              printf("\n");
       }
        
      
       
       return 0;
}
